<?xml version="1.0" encoding="UTF-8"?>

<odoo>
    <data>
        <record id="view_account_invoice_tree_inherit" model="ir.ui.view">
            <field name="name">account.invoice.tree.inherit</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_invoice_tree"/>
            <field name="arch" type="xml">
                <tree >
                    <header>
                        <button name="export_efaktur_csv" string="Download CSV" type="object" class="oe_highlight"/>
                    </header>
                </tree>
            </field>
        </record>

        <record id="inherit_menu_efaktur" model="ir.ui.menu">
            <field name="name">Set Tax Id Range</field>
            <field name="sequence" type="int">10</field>
            <field name="parent_id" ref="efaktur.efaktur_menu_id"/>
            <field name="action" ref="l10n_id_efaktur.efaktur_invoice_action"/>
        </record>
    </data>
</odoo>


